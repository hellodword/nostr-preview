// cSpell Settings
{
  // Version of the setting file.  Always 0.2
  "version": "0.2",
  // language - current active spelling language
  "language": "en",
  "ignorePaths": [
    ".devcontainer",
    "yarn.lock"
  ],
  "useGitignore": true,
  // words - list of words to be always considered correct,
  "words": [
    "devcontainer",
    "nostr",
    "vuetify",
    "roboto",
    "fontface",
    "webfontloader",
    "materialdesignicons",
    "Composables",
    "vite",
    "linkify",
    "damus",
    "mathjax"
  ]
}